      <lg><l xml:id="v1" n="v1"><pb>9r</pb> Einer rede ich hie beginne</l><l xml:id="v2" n="v2">in ſant <rub>Marien</rub> minne</l><l xml:id="v3" n="v3">der æwig<abbr>en</abbr> chvniginne</l><l xml:id="v4" n="v4"><rub>D</rub>iv ger<sup>uo</sup>che mine ſinne</l><l xml:id="v5" n="v5"><rub>v</rub><abbr>nd</abbr> mine bruſt erreinen</l><l xml:id="v6" n="v6"><rub>U</rub>on aller ſlahte mainen</l><l xml:id="v7" n="v7"><rub>D</rub>az ich nu m<sup>ve</sup>zze ſchr<add>e</add>iben</l><l xml:id="v8" n="v8"><rub>v</rub>on ir div allen weiben</l><l xml:id="v9" n="v9"><rub>d</rub>iv itewiz hat benomen</l><l xml:id="v10" n="v10"><rub>D</rub>az der t<sup>oe</sup>t waz bechomen</l><l xml:id="v11" n="v11"><rub>U</rub>on dem ærſten wibe in die welt</l><l xml:id="v12" n="v12"><rub>D</rub>o wart div aewige magt erwelt</l><l xml:id="v13" n="v13"><rub>D</rub>az beidiv man vnde weip</l><l xml:id="v14" n="v14"><rub>L</rub>ieht v<abbr>nd</abbr> den æwigen leip</l><l xml:id="v15" n="v15"><rub>m</rub>it ziêrde m<sup>ue</sup>zze ſchowen</l><l xml:id="v16" n="v16"><rub>v</rub>on der tivren vrouwen</l><l xml:id="v17" n="v"/><l xml:id="v18" n="v17"><rub>S</rub>i gebar die sch<sup>oe</sup>nen ſvnne </l><l xml:id="v19" n="v18"><rub>S</rub>i iſt aller werlde wunne</l><l xml:id="v20" n="v19"><rub>W</rub>ie wol ſiz allez vnder vienk</l><l xml:id="v21" n="v20"><rub>S</rub>waz ſvnden eva begienk</l><l xml:id="v22" n="v21"><pb>9v</pb> <rub>S</rub>i hat verb<sup>ue</sup>zzet wol den val</l><l xml:id="v23" n="v22"><rub>I</rub>r chevſche lævhtet vber al</l><l xml:id="v24" n="v23"><rub>N</rub>v ſul wir ir getrowen <del>wol</del></l><l xml:id="v25" n="v24"><rub>W</rub>an wir die ſvnde bowen</l><l xml:id="v26" n="v25"><rub>D</rub>az ſi vns ellende</l><l xml:id="v27" n="v26"><rub>F</rub>r<sup>oe</sup>lich heim ſende</l><l xml:id="v28" n="v27"><rub>U</rub>nde daz ſi weiſe </l><l xml:id="v29" n="v28"><rub>Z</rub><sup>uo</sup> dem fr<sup>oe</sup>nen paradeiſe</l><l xml:id="v30" n="v29"><rub>D</rub>a fro eva ovz geviel</l><l xml:id="v31" n="v30"><rub>v</rub>on dem mêre chan ſi den chiel</l><l xml:id="v32" n="v31"><rub>h</rub>arte wol geleiten</l><l xml:id="v33" n="v32"><rub>o</rub><add>v</add>z diſen arebeiten</l><l xml:id="v34" n="v33"><rub>Z</rub><sup>vo</sup> der himel porten</l><l xml:id="v35" n="v34"><rub>M</rub>it den gotes worten</l><l xml:id="v36" n="v35"><rub>I</rub>r wart div warheit geſagt</l><l xml:id="v37" n="v36"><rub>S</rub>i iſt m<sup>uo</sup>ter v<abbr>nd</abbr> magt</l><l xml:id="v38" n="v37"><rub>S</rub>i hat der engel niht betrogen</l><l xml:id="v39" n="v38"><rub>G</rub>ot hat daz ſpunne geſogen</l><l xml:id="v40" n="v39"><rub>A</rub>b ir vil ædelen prust</l><l xml:id="v41" n="v40"><rub>D</rub>ie nie mannes geluſt</l><l xml:id="v42" n="v41"><rub>G</rub>ewan noch der ſvnden val</l><l xml:id="v43" n="v42"><rub>S</rub>i ist m<sup>uo</sup>ter ane mal</l><l xml:id="v44" n="v43"><pb>10r</pb> Von <unclear>æ^</unclear>wen ze <unclear>æ^</unclear>wen</l><l xml:id="v45" n="v44">Wir ſulen ſey an r<sup>ue</sup>fen v<abbr>nd</abbr> flêgen </l><l xml:id="v46" n="v45">HImeliſche vrowe</l><l xml:id="v47" n="v46">mit geiſtlichem t<sup>ov</sup>we</l><l xml:id="v48" n="v47">begivz minen gedanch</l><l xml:id="v49" n="v48">daz ich din lob vnt dinen ſanch</l><l xml:id="v50" n="v49">ein teil gemæren m<sup>vu</sup>zze</l><l xml:id="v51" n="v50"><rub>v</rub><abbr>nd</abbr> hilf daz ich geb<sup>ue</sup>zze</l><l xml:id="v52" n="v51">daz vnreht daz ich ie begie</l><l xml:id="v53" n="v52">wan ich gezweivelt nie</l><l xml:id="v54" n="v53">an dir noch an dem ſune dein</l><l xml:id="v55" n="v"/><l xml:id="v56" n="v54">æwiges magedin</l><l xml:id="v57" n="v55">la daz ich geniezze </l><l xml:id="v58" n="v56">daz dv nie v<abbr>er</abbr>liezze</l><l xml:id="v59" n="v57">den heinen yrdiſch<abbr>en</abbr> man</l><l xml:id="v60" n="v58">der dich ze votinne wolde han</l><l xml:id="v61" n="v59"><rub>D</rub>v biſt lieht<abbr>er</abbr> dan der tak</l><l xml:id="v62" n="v60">v<abbr>nd</abbr> aller tugende ſmak</l><l xml:id="v63" n="v61">flivzzet ovz diner ſch<sup>oe</sup>zzen</l><l xml:id="v64" n="v62">bedenche die êre grozzen</l><l xml:id="v65" n="v63">die dir der engel brahte</l><l xml:id="v66" n="v64">do got an dich gedahte</l><l xml:id="v67" n="v65">fur aller welde n<sup>oe</sup>t </l><l xml:id="v68" n="v66">v<abbr>nd</abbr> dir den gr<sup>uo</sup>z enbot</l><l xml:id="v69" n="v67">in ditz iamer tal</l><l xml:id="v70" n="v68">daz er dich wolde ze einem ſal</l><l xml:id="v71" n="v69">ſin ſelbez libe ze weihen</l><l xml:id="v72" n="v70">wie <pb>10v</pb> mohteſt dv mich ver zeihen</l><l xml:id="v73" n="v71">g<sup>uo</sup>ter rede v<abbr>nd</abbr> liſte</l><l xml:id="v74" n="v72">du biſt div oberiſte</l><l xml:id="v75" n="v73">nach gotes manchrefte </l><l xml:id="v76" n="v74">ditz liet ich anehefte</l><l xml:id="v77" n="v75"><sup>ov</sup>f dine genade volle</l><l xml:id="v78" n="v76">dv biſt div tovwige wolle</l><l xml:id="v79" n="v77">Nv wil ich ev den orthaben</l><l xml:id="v80" n="v78">bede chvnden v<abbr>nd</abbr> ſagen</l><l xml:id="v81" n="v79">daz ich die materye</l><l xml:id="v82" n="v80">mit himiliſchem herie</l><l xml:id="v83" n="v81">v<abbr>nd</abbr> die ſtarken mêre</l><l xml:id="v84" n="v82">dest<abbr>er</abbr> baz bew<unclear>æ^</unclear>re</l><l xml:id="v85" n="v83">Math<abbr>eu</abbr>s ewangeliſte </l><l xml:id="v86" n="v84">d<abbr>er</abbr> ſchreib ez von chriſte</l><l xml:id="v87" n="v85">v<abbr>nd</abbr> von d<abbr>er</abbr> magt div in tr<sup>uo</sup>k</l><l xml:id="v88" n="v86">da von er zeichen ſchreib gen<sup>uo</sup>ch</l><l xml:id="v89" n="v87">doch waz div red bedwungen</l><l xml:id="v90" n="v88">in ebreyſcher zvngen</l><l xml:id="v91" n="v89">vntz an ſant Jeronim<unclear>u</unclear>m</l><l xml:id="v92" n="v90">der tet daz durt den gotes ſvn</l><l xml:id="v93" n="v91">v<abbr>nd</abbr> durch zweyer piſchof rat</l><l xml:id="v94" n="v92">daz er ditz liet getihtet hat</l><l xml:id="v95" n="v93">in die ſenfte lateine</l><l xml:id="v96" n="v94">daz wazzer wart ze weine</l><l xml:id="v97" n="v95">div milich wart ze <sup>oe</sup>le</l><l xml:id="v98" n="v96">do er vns ſchreib ſo wole</l><l xml:id="v99" n="v"/><l xml:id="v100" n="v"/><l xml:id="v101" n="v"/><l xml:id="v102" n="v97">dez heten in geſc<unclear>h</unclear>undet</l><l xml:id="v103" n="v98">daz er die red chundet</l><l xml:id="v104" n="v99"><rub>F</rub>romaci<abbr>us</abbr> v<abbr>nd</abbr> <rub>E</rub>liodor<abbr>us</abbr></l><l xml:id="v105" n="v100">die pyſcof hiezzen <pb>11r</pb> alſus</l><l xml:id="v106" n="v101">die im ſanten ir brief</l><l xml:id="v107" n="v102">daz e die ſchrift div vor ſlief</l><l xml:id="v108" n="v103">mit predige ſolde weche</l><l xml:id="v109" n="v104">die s<sup>ve</sup>zzen lere entechen</l><l xml:id="v110" n="v105">die ê waz beſchatot</l><l xml:id="v111" n="v106">daz die gotes chint ir brot</l><l xml:id="v112" n="v107">v<abbr>nd</abbr> ir ſpeiſe funden daran</l><l xml:id="v113" n="v108">der ingewunden van</l><l xml:id="v114" n="v109">d<abbr>er</abbr> wart da wol gelaitet</l><l xml:id="v115" n="v110">v<abbr>nd</abbr> fur daz hêr gepreitet</l><l xml:id="v116" n="v111">daz div chriſtenliche ſchar</l><l xml:id="v117" n="v112">mit chreften muge varen dar</l><l xml:id="v118" n="v113">ze geiſtlichem ſturme</l><l xml:id="v119" n="v114">gen dem lintwurme</l><l xml:id="v120" n="v115">da der ſich ſol erg<unclear>æ^</unclear>n </l><l xml:id="v121" n="v116">von d<abbr>er</abbr> vinst<abbr>er</abbr>er ſul wir erſtên</l><l xml:id="v122" n="v117">z<sup>uo</sup> dem <unclear>e</unclear>wigen liehte</l><l xml:id="v123" n="v118">got geſchvf vns von nihte</l><l xml:id="v124" n="v119">der iſt vnſer vater g<sup>uo</sup>t<abbr>er</abbr></l><l xml:id="v125" n="v120">nv bite wir ſine m<sup>uo</sup>ter</l><l xml:id="v126" n="v121">daz ſi vnſ wol beh<sup>ve</sup>te</l><l xml:id="v127" n="v122">mit ir t<sup>ov</sup>ſ<abbr>en</abbr>tfalte g<sup>ue</sup>te</l><l xml:id="v128" n="v123">Der brieſt<abbr>er</abbr> heizzet Jeronimus</l><l xml:id="v129" n="v124">der im gezimbert hat <unclear/> daz hovs</l><l xml:id="v130" n="v125">mit diſem b<sup>uo</sup>che hin ze got</l><l xml:id="v131" n="v126">swaz <rub>Math</rub><abbr>eu</abbr>s<wrong_markup>d</wrong_markup><abbr>er</abbr> frone bot</l><l xml:id="v132" n="v127">den ebreyſchen vor ſaget</l><l xml:id="v133" n="v128">dirre herre dez niht verdaget</l><l xml:id="v134" n="v129">inder lateine</l><l xml:id="v135" n="v130"><pb>11v</pb> von dem æwige ſcheine</l><l xml:id="v136" n="v131">er chunde vil wol choſen</l><l xml:id="v137" n="v132">von d<abbr>er</abbr> lylien v<abbr>nd</abbr> d<abbr>er</abbr> roſen</l><l xml:id="v138" n="v133">div den dorn niht enhat</l><l xml:id="v139" n="v134">nv wolde ich ir rat</l><l xml:id="v140" n="v135">mit ir helfe ſ<sup>uo</sup>chen</l><l xml:id="v141" n="v136">ob ſi des wolt ger<sup>uo</sup>chen</l><l xml:id="v142" n="v137">daz ich in tevſcher re<lig>de</lig></l><l xml:id="v143" n="v138">daz p<sup>uo</sup>ch prehte her zewêge</l><l xml:id="v144" n="v139">daz ez alle mugen leſen</l><l xml:id="v145" n="v140">die goteſ chint well<abbr>en</abbr> weſen</l><l xml:id="v146" n="v141">v<abbr>nd</abbr> auch mugen ſchowen</l><l xml:id="v147" n="v142">phaffen layen vrowen</l><l xml:id="v148" n="v"/><l xml:id="v149" n="v"/><l xml:id="v150" n="v143">wie daz kint die m<sup>uo</sup>t<abbr>er</abbr> chos</l><l xml:id="v151" n="v144">div den magt<sup>vo</sup>m nie v<abbr>er</abbr>los</l><l xml:id="v152" n="v145">noch nimm<abbr>er</abbr> mak v<abbr>er</abbr>lieſen</l><l xml:id="v153" n="v146">wir mugen wol kyeſen</l><l xml:id="v154" n="v147">wie genedik ſi ſey</l><l xml:id="v155" n="v148">der daz kint ſitzet bey</l><l xml:id="v156" n="v149">daz lêwe v<abbr>nd</abbr> lamp iſt</l><l xml:id="v157" n="v150">ob allen dingen oberiſt</l><l xml:id="v158" n="v151">bede læwen v<abbr>nd</abbr> t<sup>oe</sup>t</l><l xml:id="v159" n="v"/><l xml:id="v160" n="v152">herre fleiſch v<abbr>nd</abbr> pr<sup>oe</sup>t</l><l xml:id="v161" n="v"/><l xml:id="v162" n="v"/><l xml:id="v163" n="v"/><l xml:id="v164" n="v"/><l xml:id="v165" n="v153">vat<abbr>er</abbr> vnde ſvn</l><l xml:id="v166" n="v154">aynvalt v<abbr>nd</abbr> weiſtum</l><l xml:id="v167" n="v155">groz vnde chleine</l><l xml:id="v168" n="v156">iſt er alterſeine</l><l xml:id="v169" n="v157">der vns ze notdurfte erſchein</l><l xml:id="v170" n="v158">er nam hie fleiſch v<abbr>nd</abbr> payn</l><l xml:id="v171" n="v"/><l xml:id="v172" n="v"/><l xml:id="v173" n="v"/><l xml:id="v174" n="v"/><l xml:id="v175" n="v"/><l xml:id="v176" n="v"/><l xml:id="v177" n="v159">ſin ſneweizze ſæle</l><l xml:id="v178" n="v160">f<sup>uo</sup>r in die phalzen hêre</l><l xml:id="v179" n="v161">ſande MaRien</l><l xml:id="v180" n="v162">die wold vns wol freyen</l><l xml:id="v181" n="v163">von den ſchæmelichen iochen</l><l xml:id="v182" n="v164">di<unclear>s</unclear> cheten iſt zebrochen</l><l xml:id="v183" n="v"/><l xml:id="v184" n="v165"><rub>D</rub>a vns d<abbr>er</abbr> tievel mit pant</l><l xml:id="v185" n="v166">des lobe <pb>12r</pb> wir den hailant <rub>AmeN</rub></l><l xml:id="v186" n="v"/><l xml:id="v187" n="v"/><l xml:id="v188" n="v"/><l xml:id="v189" n="v"/><l xml:id="v190" n="v167">DEn haz ich hie v<abbr>er</abbr>dinge</l><l xml:id="v191" n="v168">v<abbr>nd</abbr> neit vntz ich furbringe</l><l xml:id="v192" n="v169">diſe ſæligen wort  </l><l xml:id="v193" n="v170">waz Math<abbr>eu</abbr>s ſchreib dort</l><l xml:id="v194" n="v171">den ebrayſch<abbr>en</abbr> levten</l><l xml:id="v195" n="v172">daz wil ich iv bedevten</l><l xml:id="v196" n="v173">ſagen v<abbr>nd</abbr> ſchreiben</l><l xml:id="v197" n="v174"><unclear>man</unclear>en v<abbr>nd</abbr> weiben</l><l xml:id="v198" n="v175">mit goteſ volleiſt</l><l xml:id="v199" n="v176">v<abbr>nd</abbr> dem heiligen geiſt</l><l xml:id="v200" n="v"/><l xml:id="v201" n="v"/><l xml:id="v202" n="v"/><l xml:id="v203" n="v"/><l xml:id="v204" n="v"/><l xml:id="v205" n="v"/><l xml:id="v206" n="v"/><l xml:id="v207" n="v"/><l xml:id="v208" n="v"/><l xml:id="v209" n="v"/><l xml:id="v210" n="v"/><l xml:id="v211" n="v"/><l xml:id="v212" n="v177">ſwer ditz liet v<abbr>er</abbr>ſpreche</l><l xml:id="v213" n="v178"><rub>Math</rub><abbr>eu</abbr>s<wrong_markup>m</wrong_markup><sup>vo</sup>z ez rechen</l><l xml:id="v214" n="v179">d<abbr>er</abbr>z zemerſt<abbr>en</abbr> ſchreip</l><l xml:id="v215" n="v180">v<abbr>nd</abbr> <unclear>den</unclear> irſal v<abbr>er</abbr>treip</l><l xml:id="v216" n="v"/><l xml:id="v217" n="v"/><l xml:id="v218" n="v"/><l xml:id="v219" n="v"/><l xml:id="v220" n="v"/><l xml:id="v221" n="v"/><l xml:id="v222" n="v"/><l xml:id="v223" n="v"/><l xml:id="v224" n="v"/><l xml:id="v225" n="v"/><l xml:id="v226" n="v"/><l xml:id="v227" n="v"/><l xml:id="v228" n="v181">Math<abbr>eu</abbr>s ewa<add>n</add>gel<abbr>i</abbr>ſte</l><l xml:id="v229" n="v182">d<abbr>er</abbr> nam im lange friſte</l><l xml:id="v230" n="v183">vnz er ſich vil rehte erf<sup>uo</sup>r</l><l xml:id="v231" n="v184">daz wed<abbr>er</abbr> mos noch m<sup>uo</sup>r</l><l xml:id="v232" n="v185">ſine wort mak getr<sup>ve</sup>ben</l><l xml:id="v233" n="v186">des vns wol ſol gen<sup>ve</sup>gen</l><l xml:id="v234" n="v187">an ſant <rub>J</rub>eronimi lære</l><l xml:id="v235" n="v"/><l xml:id="v236" n="v188">d<abbr>er</abbr> ger<sup>uo</sup>che wende vnſer ſære</l><l xml:id="v237" n="v189"><unclear>indem</unclear>goteſ na<abbr>men</abbr></l><l xml:id="v238" n="v190">daz geſchehe amen</l><l xml:id="v239" n="v191">Bey den alten zeit<abbr>en</abbr></l><l xml:id="v240" n="v192">was got niht chunt weit<abbr>en</abbr></l><l xml:id="v241" n="v193">niw<abbr>er</abbr> in Jvdea</l><l xml:id="v242" n="v194">fromde was er anderſwa</l><l xml:id="v243" n="v195">do was d<abbr>er</abbr> heidenſchaft ſo vil</l><l xml:id="v244" n="v196">die des tievels ſpil</l><l xml:id="v245" n="v197">anden apgoten begiengen</l><l xml:id="v246" n="v198">deſ ſi auch ſchade enphiengen</l><l xml:id="v247" n="v199">an dem leibe v<abbr>nd</abbr> an d<abbr>er</abbr> ſæle</l><l xml:id="v248" n="v200">do behielt div goteſ lære</l><l xml:id="v249" n="v201">div iſr<abbr>ahel</abbr>iſche diet</l><l xml:id="v250" n="v202">als in <rub>M</rub>oyſes geriet</l><l xml:id="v251" n="v203">v<abbr>nd</abbr> ir vat<abbr>er</abbr> <rub>a</rub>braham</l><l xml:id="v252" n="v204">dem waren ſi gehorſam</l><l xml:id="v253" n="v205"><rub>Y</rub>ſaac v<abbr>nd</abbr> jacobe</l><l xml:id="v254" n="v"/><l xml:id="v255" n="v"/><l xml:id="v256" n="v"/><l xml:id="v257" n="v"/><l xml:id="v258" n="v"/><l xml:id="v259" n="v206">d<abbr>er</abbr> in deſ himels hoher <unclear>æ^</unclear></l><l xml:id="v260" n="v207"><pb>12v</pb> ein leitern geſach </l><l xml:id="v261" n="v208">v<abbr>nd</abbr> in ſin ſchêpher ſach</l><l xml:id="v262" n="v209">facie ad faciem</l><l xml:id="v263" n="v210">wie moht ez im baz ergen</l><l xml:id="v264" n="v211">do er den engel ge vie</l><l xml:id="v265" n="v212">den er des morgens niht ver lie</l><l xml:id="v266" n="v213">vnz er in geſegenot</l><l xml:id="v267" n="v"/><l xml:id="v268" n="v214">div hilf wont im niht bey genot</l><l xml:id="v269" n="v215">do in der engel drukte</l><l xml:id="v270" n="v216">hin naher er ſich rukte</l><l xml:id="v271" n="v217">ze einem vr chunde </l><l xml:id="v272" n="v218">hinchen er beg<abbr>un</abbr>de</l><l xml:id="v273" n="v219">der heilige pa<abbr>tri</abbr>arch</l><l xml:id="v274" n="v220">deſ wundert vil ſtarch</l><l xml:id="v275" n="v221">alles ſin gesſlæhte</l><l xml:id="v276" n="v222">als ez von ſchulden t<sup>uo</sup>n mohte</l><l xml:id="v277" n="v223">ſi wurden deſ gefrevt</l><l xml:id="v278" n="v224">daz er wart beſchout</l><l xml:id="v279" n="v225">von gotes anplike</l><l xml:id="v280" n="v226">ſi ſ<sup>uo</sup>chten venig dike</l><l xml:id="v281" n="v227">gen den himel ch<sup>oe</sup>ren </l><l xml:id="v282" n="v228">hie mvgt ir wol wunder horen</l><l xml:id="v283" n="v229"><rub>O</rub>uz dem chunne zierſam</l><l xml:id="v284" n="v230">wart ein chint lobeſam</l><l xml:id="v285" n="v231">indise w<abbr>er</abbr>lt geborn</l><l xml:id="v286" n="v232">got ſelbe het im erchorn</l><l xml:id="v287" n="v233">ſin gedanch v<abbr>nd</abbr> ſin<abbr>en</abbr> ſin</l><l xml:id="v288" n="v234">ge heizzen wart er <rub>J</rub>oachim</l><l xml:id="v289" n="v235">d<abbr>er</abbr> waz der beſten ayn</l><l xml:id="v290" n="v236">den div ſvnne ie beſchein</l><l xml:id="v291" n="v237">sin g<sup>ue</sup>te waz ſo groz</l><l xml:id="v292" n="v238">daz er ſin wol genoz</l><l xml:id="v293" n="v239">vor got v<abbr>nd</abbr> vor den levten</l><l xml:id="v294" n="v240">Joachim ſ<abbr>pri</abbr>chet zebedeuten</l><l xml:id="v295" n="v241"><rub>R</rub>e<abbr>par</abbr>a<abbr>ci</abbr>o d<abbr>omin</abbr>i</l><l xml:id="v296" n="v242">ſwa ez geſchriben ſey</l><l xml:id="v297" n="v243">daz ſ<abbr>pri</abbr>chet vnſers herren beraitunge</l><l xml:id="v298" n="v244">liehtvaz der ſunne</l><l xml:id="v299" n="v"/><l xml:id="v300" n="v"/><l xml:id="v301" n="v245">wan er <pb>13r</pb> der mægede vat<abbr>er</abbr> wart </l><l xml:id="v302" n="v246">div dem engel waz ſo zart</l><l xml:id="v303" n="v247">daz ſi bey den pl<sup>uo</sup>men</l><l xml:id="v304" n="v"/><l xml:id="v305" n="v248">mit ſpile wolden r<sup>uo</sup>ben</l><l xml:id="v306" n="v249">vnz ſi m<sup>uo</sup>t<abbr>er</abbr> wurde</l><l xml:id="v307" n="v250">d<abbr>er</abbr> gotlichen burde</l><l xml:id="v308" n="v251">v<abbr>nd</abbr> auch mit æren ſolde ſin</l><l xml:id="v309" n="v252">des himilreiches turlin</l><l xml:id="v310" n="v"/><l xml:id="v311" n="v"/><l xml:id="v312" n="v"/><l xml:id="v313" n="v"/><l xml:id="v314" n="v253">v<abbr>nd</abbr> div wol gereiniget cham<abbr>er</abbr></l><l xml:id="v315" n="v254">div ger<sup>uo</sup>che vnſ zeſamene </l><l xml:id="v316" n="v255">fur ir ſun <abbr>bri</abbr>ngen</l><l xml:id="v317" n="v256">ſi ist all<abbr>er</abbr> æren gimme</l><l xml:id="v318" n="v257">Joachym der gotes man</l><l xml:id="v319" n="v"/><l xml:id="v320" n="v"/><l xml:id="v321" n="v"/><l xml:id="v322" n="v"/><l xml: